{
    "phpactor.enable": true,
    "phpactor.path": "/usr/local/bin/phpactor",
    "languageserver": {
        "phpactor": {
            "command": "phpactor",
            "args": ["language-server"],
            "trace.server": "verbose",
            "filetypes": ["php"]
        }
    },
    "tailwindCSS.headwind.runOnSave": true
}
